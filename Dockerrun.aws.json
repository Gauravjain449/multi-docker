{
  "AWSEBDockerrunVersion": 2,
  "containerDefinations": [
    {
      "name": "client",
      "image": "gauravjain449/multi-client",
      "hostname": "client",
      "essential": false
    },
    {
      "name": "server",
      "image": "gauravjain449/multi-server",
      "hostname": "api",
      "essential": false
    },
    {
      "name": "worker",
      "image": "gauravjain449/multi-worker",
      "hostname": "worker",
      "essential": false
    },
    {
      "name": "nginx",
      "image": "gauravjain449/multi-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "conatainerPort": 80
        }
      ],
      "links": ["client", "server"]
    }
  ]
}
